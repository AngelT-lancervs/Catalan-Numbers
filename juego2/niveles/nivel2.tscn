script/source = "extends Node2D

@onready var player = $Player2
var matrix = []
var movimientos : int = 0
@onready var labelMov = $GUI/Control/mov/movRestantes
var numMoves

# Called when the node enters the scene tree for the first time.
func _ready():
	player.animation.play(\"idle\")
	$fairy/AnimatedSprite2D.play(\"default\")
	crearMatrix(4)
	numMoves = 6

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	# Check if the player is below the diagonal
	if player != null: 
		var playerPosition = player.position
		var matrixPosition = worldToMatrix(playerPosition)
		print(matrixPosition)
		print(player.position.y)
		labelMov.text = str(numMoves - player.movimientos.size())
		gameOver()

func crearMatrix(n):
	for x in range(n):
		matrix.append([])
		for y in range(n):
			matrix[x].append(0)

func worldToMatrix(worldPos):
	# Convert world position to matrix indices
	var tileSize = 128 # Adjust this to your tile size
	var matrixX = int(worldPos.x / tileSize)
	var matrixY = int(worldPos.y / tileSize)
	return Vector2(matrixX, matrixY)
	

func _on_pared_body_entered(body):
	body.speedX = 0
	body.position.x = body.position.x - 25.0003
	await get_tree().create_timer(1).timeout
	body.speedX = 100
	player.movimientos.pop_back()
	


func _on_diagonal_body_entered(body):
	body.speedY = 0
	body.position.y = body.position.y + 24.5
	await get_tree().create_timer(1).timeout
	body.speedY = 100
	player.movimientos.pop_back()

func gameOver():
	if (player.health <= 0 and !player.corriendoDerecha and !player.corriendoArriba) or (numMoves - player.movimientos.size() == 0 and !player.corriendoDerecha and !player.corriendoArriba):
		$GameOver.visible = true
		player.speedX = 0
		player.speedY = 0
		$GUI.visible = false
		player.animation.play(\"die\")
		await player.animation.animation_finished
		player.queue_free()

func _on_fairy_body_entered(body):
	TRANSITION.changeEscena(\"res://juego2/niveles/nivel3.tscn\")
	$fairy/AudioStreamPlayer.play()


func _on_ost_finished():
	$Sounds/ost.play()
"

