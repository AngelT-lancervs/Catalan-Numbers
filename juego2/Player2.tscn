[sub_resource type="GDScript" id="GDScript_6qy3i"]
resource_name = "playerScript"
script/source = "extends CharacterBody2D

var idle : bool
var corriendoDerecha: bool
var corriendoArriba: bool
var speedX = 100
var speedY = 100
var movimientoMax : int = 0
var movimientos : Array[int] #0 derecha , 1 arriba

@onready var animation = $animaciones

func _ready():
	idle = true
	corriendoDerecha = false
	corriendoArriba = false

func _physics_process(delta):
	print(movimientos)
	correrDerecha()
	correrArriba()
	move_and_slide()

func correrDerecha():
	if Input.is_action_just_pressed(\"ui_right\") && !corriendoDerecha:
		movimientoMax = 0 
		corriendoDerecha = true
		if speedX > 0:
			movimientos.append(0)
	if corriendoDerecha:
		if movimientoMax <= 148:
			velocity.x = 1 * speedX
			movimientoMax += 2
			animation.play(\"run\")
			return true
		else:
			velocity.x = 0
			corriendoDerecha = false
			animation.play(\"idle\")
			return false



func correrArriba():
	if Input.is_action_just_pressed(\"ui_up\") && !corriendoArriba:
		movimientoMax = 0 
		corriendoArriba = true
		if speedY > 0:
			movimientos.append(1)
	if corriendoArriba:
		if movimientoMax <= 148:
			velocity.y = -1 * speedY
			movimientoMax += 2
			animation.play(\"run\")
			return true
		else:
			velocity.y = 0
			corriendoArriba = false
			animation.play(\"idle\")
			return false



"

